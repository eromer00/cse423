  frozenOnTheCuspOfForeverTheSpacecraftHungLikeATimelessDewdropOnTheLeavesOfSpace()
{
   int a, b, c;

   a=b=c=0;
   while (a<3) {
       while (b<3) {
	   while (c<3) {
	       c++;
	       if (c==2) break;
	   }
	   b++;
	   if (b==2) break;
       }
       a++;
       if (a==2) break;
   }
   if (a) break;
   if (b) c;
   else break;
}
